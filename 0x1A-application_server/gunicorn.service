# Create a service for web-01, to be used by gunicorn
[Unit]
Description="Gunicorn instance to serve web-01"
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
Environment="PATH=/usr/bin/python3"
ExecStart=/home/ubuntu/.local/bin/gunicorn  --workers 3 --bind 0.0.0.0:5003 -m 007 web_dynamic.2-hbnb:app >> /tmp/airbnb-access.log 2> /tmp/airbnb-error.logirbnb-acces>
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target









